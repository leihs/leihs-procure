{
  "name": "leihs-procurement-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "serve": "vite preview",
    "build": "npm run -s build:i18n && npm run -s build:app",
    "build:app": "vite build",
    "build:i18n": "node src/locale/make-pseudoloc.js",
    "postinstall": "npm run cleanup-typescript",
    "prestart": "npm run -s build:i18n",
    "pretest": "npm run -s build:i18n",
    "cleanup-typescript": "find . -type f -name '*.ts' -or -name '*.tsx' | xargs rm",
    "analyze": "source-map-explorer build/static/js/main.*",
    "analyze-build": "npm run -s build && npm run -s analyze",
    "lint": "npx eslint --fix 'src/' && npm run -s lint:json",
    "ci:lint": "npx eslint 'src/' && npm run -s ci:lint:json",
    "lint:json": "npx prettier --write 'src/{,**/}*.json'",
    "ci:lint:json": "npx prettier --list-different 'src/{,**/}*.json'",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "snapshots": "vitest -u"
  },
  "dependencies": {
    "@apollo/client": "^3.8.4",
    "@fortawesome/fontawesome-free-solid": "^5.1.0-3",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-regular-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@popperjs/core": "^2.11.8",
    "@types/node": "^20.5.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "accounting-js": "^1.1.1",
    "bootstrap": "^4.2.1",
    "browser-info": "^1.2.0",
    "classnames": "^2.3.2",
    "cross-fetch": "^4.0.0",
    "date-fns": "^2.30.0",
    "debug": "^3.2.6",
    "downshift": "^1.31.16",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "focus-trap-react": "^10.2.3",
    "graphiql": "^1.11.5",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.10.0",
    "lodash": "^4.17.23",
    "luxon": "^1.9.0",
    "moment": "^2.29.4",
    "prop-types": "^15.6.2",
    "pseudoloc": "^1.1.0",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-day-picker": "^8.9.1",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.11.0",
    "react-popper": "^2.3.0",
    "react-router": "^6.30.2",
    "react-router-dom": "^6.30.2",
    "react-test-renderer": "^18.2.0",
    "reactstrap": "^8.10.1",
    "source-map-explorer": "^2.5.3",
    "tachyons-type-scale": "^6.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.4.21",
    "vite-plugin-node-polyfills": "^0.14.1",
    "vite-plugin-svgr": "^3.2.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.3/xlsx-0.19.3.tgz"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint-plugin-vitest": "^0.5.0",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "jsdom": "^22.1.0",
    "vitest": "^1.6.0"
  },
  "overrides": {
    "qs": ">=6.14.2",
    "esbuild": ">=0.25.0"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "vitest-globals/env": true
    },
    "plugins": [
      "react",
      "vitest"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:vitest/recommended",
      "plugin:vitest-globals/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "warn",
      "vitest/prefer-to-be": "off"
    }
  }
}
